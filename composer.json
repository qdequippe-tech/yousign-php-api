{
    "name": "qdequippe/yousign-php-api",
    "description": "PHP Yousign Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Quentin Dequippe",
            "email": "quentin@dequippe.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "Qdequippe\\Yousign\\Api\\": "generated/"
        }
    },
    "require": {
        "php": ">= 8.1",
        "jane-php/open-api-runtime": "^7.6"
    },
    "require-dev": {
        "jane-php/open-api-3": "^7.6",
        "friendsofphp/php-cs-fixer": "^3.56",
        "rector/rector": "^1.0 || ^2.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
